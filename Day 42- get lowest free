# Read number of test cases
T = int(input())

# Process each test case
for _ in range(T):
    # Read the prices A, B, C
    A, B, C = map(int, input().split())
    
    # Calculate the total cost Chef needs to pay
    total_cost = A + B + C - min(A, B, C)
    
    # Output the result
    print(total_cost)
